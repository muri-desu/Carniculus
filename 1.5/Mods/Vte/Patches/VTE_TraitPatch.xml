<?xml version="1.0" encoding="utf-8" ?>
<Patch>
<Operation Class="PatchOperationSequence">
	<operations>

	<li Class="PatchOperationAdd">
		<xpath>Defs/AlienRace.AlienBackstoryDef[defName = "MazeRunner"]/forcedTraitsChance</xpath>
		<value>
			<li><defName>VTE_Eccentric</defName><chance>50</chance></li>
		</value>
	</li>

	<li Class="PatchOperationAdd">
		<xpath>Defs/AlienRace.AlienBackstoryDef[defName = "E_TestSub"]/forcedTraitsChance</xpath>
		<value>
			<li><defName>VTE_Desensitized</defName><chance>50</chance></li>
		</value>
	</li>

	<li Class="PatchOperationAdd">
		<xpath>Defs/AlienRace.AlienBackstoryDef[defName = "PlayingDead"]/forcedTraitsChance</xpath>
		<value>
			<li><defName>VTE_IronStomach</defName><chance>70</chance></li>
		</value>
	</li>

	<li Class="PatchOperationAdd">
		<xpath>Defs/AlienRace.AlienBackstoryDef[defName = "FeralChild"]/forcedTraitsChance</xpath>
		<value>
			<li><defName>VTE_Ecologist</defName><chance>70</chance></li>
		</value>
	</li>

	<li Class="PatchOperationAdd">
		<xpath>/Defs/AlienRace.AlienBackstoryDef[defName = "AlphaMale" or defName = "Matriarch" or defName = "RabidCarni"]/forcedTraitsChance</xpath>
		<value>
			<li><defName>VTE_Rebel</defName><chance>50</chance></li>
			<li><defName>VTE_ThickSkinned</defName><chance>50</chance></li>
		</value>
	</li>

	<li Class="PatchOperationAdd">
		<xpath>Defs/AlienRace.AlienBackstoryDef[defName = "Butcher"]/forcedTraitsChance</xpath>
		<value>
			<li><defName>VTE_Gastronomist</defName><chance>70</chance></li>
		</value>
	</li>

	<li Class="PatchOperationAdd">
		<xpath>/Defs/AlienRace.AlienBackstoryDef[defName = "Torturer"]/forcedTraitsChance</xpath>
		<value>
			<li><defName>VTE_MadSurgeon</defName><chance>80</chance></li>
		</value>
	</li>

	<li Class="PatchOperationAdd">
		<xpath>Defs/AlienRace.AlienBackstoryDef[defName = "Livestocker" or defName = "CattleFucker"]/forcedTraitsChance</xpath>
		<value>
			<li><defName>VTE_Menagerist</defName></li>
		</value>
	</li>

	<li Class="PatchOperationAdd">
		<xpath>Defs/AlienRace.AlienBackstoryDef[defName = "Farmhand"]/forcedTraitsChance</xpath>
		<value>
			<li><defName>VTE_Ecologist</defName></li>
		</value>
	</li>

	<li Class="PatchOperationAdd">
		<xpath>Defs/AlienRace.AlienBackstoryDef[defName = "KillerT" or defName = "Macrophage"]/forcedTraitsChance</xpath>
		<value>
			<li><defName>VTE_Brave</defName></li>
		</value>
	</li>

	<li Class="PatchOperationAdd">
		<xpath>Defs/AlienRace.AlienBackstoryDef[defName = "Constructor"]/forcedTraitsChance</xpath>
		<value>
			<li><defName>VTE_Groundbreaker</defName></li>
		</value>
	</li>

	<li Class="PatchOperationAdd">
		<xpath>Defs/AlienRace.AlienBackstoryDef[defName = "CattleKiller"]/forcedTraitsChance</xpath>
		<value>
			<li><defName>VTE_AnimalHater</defName></li>
		</value>
	</li>

	</operations>
</Operation>

</Patch>